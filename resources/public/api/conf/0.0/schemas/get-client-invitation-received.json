{
  "type": "object",
  "properties": {
    "_links": {
      "type": "object",
      "properties": {
        "reject": {
          "type": "object",
          "properties": {
            "href": {
              "type": "string",
              "description": "Link to reject the invitation"
            }
          },
          "required": [
            "href"
          ]
        },
        "accept": {
          "type": "object",
          "properties": {
            "href": {
              "type": "string",
              "description": "Link to accept the invitation"
            }
          },
          "required": [
            "href"
          ]
        },
        "self": {"$ref": "self-link.json"}
      },
      "required": [
        "reject",
        "accept",
        "self"
      ]
    },
    "regime": {
      "$ref": "regime.json"
    },
    "clientId": {
      "type": "string",
      "description": "Client's top level ID for their MTD registration (not regime specific)"
    },
    "postcode": {
      "type": "string",
      "description": "The Client's post code"
    },
    "arn": {
      "type": "string",
      "description": "The Agent Registration Number for the calling agency"
    },
    "created": {
      "type": "string",
      "description": "Creation time of the request (RFC3339 / ISO8601 format)"
    },
    "lastUpdated": {
      "type": "string",
      "description": "Last update time of the request (RFC3339 / ISO8601 format)"
    },
    "status": {"$ref": "status.json"}
  },
  "required": [
    "_links",
    "regime",
    "clientId",
    "postcode",
    "arn",
    "created",
    "lastUpdated",
    "status"
  ]
}