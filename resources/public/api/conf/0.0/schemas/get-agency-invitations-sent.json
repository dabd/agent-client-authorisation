{
  "type": "object",
  "properties": {
    "_links": {
      "type": "object",
      "description": "Related resources",
      "properties": {
        "self": {"$ref": "self-link.json"},
        "invitation": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "href": {
                "type": "string",
                "description": "An array of links to individual Invitation resources"
              }
            },
            "required": [
              "href"
            ]
          }
        }
      },
      "required": [
        "self",
        "invitation"
      ]
    },
    "_embedded": {
      "type": "object",
      "properties": {
        "invitations": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "_links": {
                "type": "object",
                "properties": {
                  "cancel": {
                    "type": "object",
                    "properties": {
                      "href": {
                        "type": "string",
                        "description": "A link to cancel to this invitation"
                      }
                    },
                    "required": [
                      "href"
                    ]
                  },
                  "self": {"$ref": "self-link.json"}
                },
                "required": [
                  "cancel",
                  "self"
                ]
              },
              "regime": {
                "$ref": "regime.json"
              },
              "clientId": {
                "type": "string",
                "description" : "Client's top level ID for their MTD registration (not regime specific)"
              },
              "postcode": {
                "type": "string",
                "description" : "The Client's post code"
              },
              "arn": {
                "type": "string",
                "description" : "The Agency Registration Number"
              },
              "created": {
                "type": "string",
                "description" : "Creation time of the request (RFC3339 / ISO8601 format)"
              },
              "lastUpdated": {
                "type": "string",
                "description" : "Last update time of the request (RFC3339 / ISO8601 format)"
              },
              "status": {"$ref": "status.json"}
            },
            "required": [
              "_links",
              "regime",
              "clientId",
              "postcode",
              "arn",
              "created",
              "lastUpdated",
              "status"
            ]
          }
        }
      },
      "required": [
        "invitations"
      ]
    }
  },
  "required": [
    "_links",
    "_embedded"
  ]
}