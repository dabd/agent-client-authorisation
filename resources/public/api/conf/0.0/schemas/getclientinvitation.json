{
  "type": "object",
  "properties": {
    "_links": {
      "type": "object",
      "properties": {
        "reject": {
          "type": "object",
          "properties": {
            "href": {
              "type": "string",
              "description": "Link to reject the invitation"
            }
          },
          "required": [
            "href"
          ]
        },
        "accept": {
          "type": "object",
          "properties": {
            "href": {
              "type": "string",
              "description": "Link to accept the invitation"
            }
          },
          "required": [
            "href"
          ]
        },
        "self": {
          "type": "object",
          "properties": {
            "href": {
              "type": "string",
              "description": "Link back to this resource"
            }
          },
          "required": [
            "href"
          ]
        }
      },
      "required": [
        "reject",
        "accept",
        "self"
      ]
    },
    "regime": {
      "type": "string",
      "description": "The tax regime specified in the invitation"
    },
    "clientId": {
      "type": "string",
      "description": "Client's top level ID for their MTD registration (not regime specific)"
    },
    "postcode": {
      "type": "string",
      "description": "This varies between regimes. In this example, for mtd-sa, it is the registered postcode of the client"
    },
    "arn": {
      "type": "string",
      "description": "The Agent Registration Number for the calling agency"
    },
    "created": {
      "type": "string",
      "description": "Creation time of the request (RFC3339 / ISO8601 format)"
    },
    "lastUpdated": {
      "type": "string",
      "description": "Creation time of the request (RFC3339 / ISO8601 format)"
    },
    "status": {
      "type": "string",
      "description": "The status of the request at the time the request was fulfilled"
    }
  },
  "required": [
    "_links",
    "regime",
    "clientId",
    "postcode",
    "arn",
    "created",
    "lastUpdated",
    "status"
  ]
}